<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}Start Bootstrap - SB Admin Version 2.0 Demo{% endblock %}</title>
    
    {#<link rel="shortcut icon" href="http://work.sopinet.es/icofrade-web/web/uploads/favicon.ico" />#}
    {% block css %}
	   	{% stylesheets
	   		'@SopinetTemplateSbadmin2Bundle/Resources/public/css/bootstrap.min.css'
	    	'@SopinetTemplateSbadmin2Bundle/Resources/public/css/plugins/morris/morris-0.4.3.min.css'
	    	'@SopinetTemplateSbadmin2Bundle/Resources/public/css/plugins/timeline/timeline.css'
	    	'@SopinetTemplateSbadmin2Bundle/Resources/public/css/sb-admin.css'
	    	output='css/*.css' filter='cssrewrite' %}
	    	<link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}" />
		{% endstylesheets %}
    {% endblock %}

</head>
<div id="wrapper">
	{% block custombody %}<body>{% endblock %}
	{% block body %}
	    <h1>Hello, world!</h1>
	{% endblock %}
</div>

{% block js %}
	{% javascripts 
		'@SopinetTemplateEndlessAdminBundle/Resources/public/js/jquery-1.10.2.js'
		'@SopinetTemplateEndlessAdminBundle/Resources/public/js/bootstrap.min.js'
		'@SopinetTemplateEndlessAdminBundle/Resources/public/js/plugins/metisMenu/jquery.metisMenu.js'
		'@SopinetTemplateEndlessAdminBundle/Resources/public/js/plugins/morris/raphael-2.1.0.min.js'
		'@SopinetTemplateEndlessAdminBundle/Resources/public/js/plugins/morris/morris.js'
		'@SopinetTemplateEndlessAdminBundle/Resources/public/js/sb-admin.js'
		'@SopinetTemplateEndlessAdminBundle/Resources/public/js/demo/dashboard-demo.js'
	%}
	<script type="text/javascript" src="{{ asset_url }}"></script>
	
	{% endjavascripts %}
{% endblock %}
    
</body>
</html>